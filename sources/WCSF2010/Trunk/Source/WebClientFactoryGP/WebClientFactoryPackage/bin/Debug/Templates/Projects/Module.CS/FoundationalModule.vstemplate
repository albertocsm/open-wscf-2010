  </WizardExtension>
  <WizardData>
    <Template xmlns="http://schemas.microsoft.com/pag/gax-template"
              SchemaVersion="1.0"
              Recipe="CreateFoundationalModuleCS">
    </Template>
  </WizardData>
</VSTemplate>
